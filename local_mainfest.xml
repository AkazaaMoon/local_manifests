<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<remote
		name="pe"
		fetch="https://github.com/PixelExperience"
		revision="thirteen"
		review="gerrit.pixelexperience.org"/>

	<remote
		name="pe-devices"
